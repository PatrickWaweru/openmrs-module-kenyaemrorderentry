<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.kenyaemrorderentry">

	<class name="org.openmrs.module.kenyaemrorderentry.LabOrderManifest"
		table="kenyaemr_order_entry_lab_manifest">
		<id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
		<property name="startDate" type="java.util.Date" column="start_date"/>
		<property name="endDate" type="java.util.Date" column="end_date"/>
		<property name="orderIdList" type ="java.lang.String" column ="order_id_list" length="255"/>
		<property name="testType" type="int" column="test_type" length="255"/>
		<property name="dateCreated" type="java.util.Date" column="date_created" length="19"/>
		<many-to-one name="changedBy" class="org.openmrs.User" column="changed_by"/>
		<property name="dateChanged" type="java.util.Date" column="date_changed" length="19"/>
		<property name="voided" type="java.lang.Boolean" column="voided" length="1"/>
		<many-to-one name="voidedBy" class ="org.openmrs.User" column="voided_by"/>
		<property name="dateVoided" type ="java.util.Date" column ="date_voided" length="19"/>
		<property name="voidReason" type="java.lang.String" column="voided_reason" length="255"/>

	</class>

</hibernate-mapping>
